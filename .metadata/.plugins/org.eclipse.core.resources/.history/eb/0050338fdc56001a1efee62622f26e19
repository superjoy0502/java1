
public class StringApp {
	public static void main(String Args[]) {
		
		// Character vs String
		System.out.println("Hello World"); // String
		System.out.println('H'); // Character
		System.out.println("H");
		
		System.out.println("Hello "
				+ "World");
		
		System.out.println("Hello /nWorld");
		
	}
}
